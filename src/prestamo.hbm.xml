<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="Objetos.Prestamo" table="prestamo">
        <meta attribute="class-description">
            This class contains the employee detail.
        </meta>
        <id name="id" type="int" column="id">
            <generator class="increment"/>
        </id>
        <property name="idLibro" column="idLibro" type="int"/>
        <property name="idSocio" column="idSocio" type="int"/>
        <property name="fechaInicio" column="fechaInicio" type="date"/>
        <property name="fechaFinal" column="fechaFinal" type="date"/>
    </class>
</hibernate-mapping>